        function openMoveStageModal() {
            selectedMoveStage = null;
            document.getElementById('moveLeadCount').textContent = selectedLeads.size;
            document.querySelectorAll('.stage-option').forEach(el => el.classList.remove('selected'));
            document.getElementById('confirmMoveBtn').disabled = true;
            document.getElementById('moveStageModal').classList.add('active');
        }

        function closeMoveStageModal() {
            document.getElementById('moveStageModal').classList.remove('active');
            selectedMoveStage = null;
        }

        function selectMoveStage(stage) {
            selectedMoveStage = stage;
            document.querySelectorAll('.stage-option').forEach(el => {
                el.classList.toggle('selected', el.dataset.stage === stage);
            });
            document.getElementById('confirmMoveBtn').disabled = false;
        }

        async function confirmBulkMove() {
            if (!selectedMoveStage || selectedLeads.size === 0) return;
            const leadIds = Array.from(selectedLeads);
            let moved = 0;
            for (const id of leadIds) {
                try {
                    const res = await fetch(`/api/leads/${id}/stage`, {
                        method: 'PUT',
                        headers: { 'Content-Type': 'application/json' },
                        body: JSON.stringify({ stage: selectedMoveStage })
                    });
                    if (res.ok) moved++;
                } catch (err) { /* continue */ }
            }
            showToast(`Moved ${moved} lead(s) to ${selectedMoveStage}`, 'success');
            closeMoveStageModal();
            clearSelection();
            setTimeout(() => window.location.reload(), 600);
        }

