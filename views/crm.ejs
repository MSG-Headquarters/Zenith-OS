        let flyerLeadId = null;
        let flyerDraftId = null;
        let flyerPhotos = [];

        async function generateDoc(leadId, type) {
            try {
                const label = type === 'loi' ? 'Letter of Intent' : 'Listing Agreement';
                showToast('Generating ' + label + '...', 'info');
                const res = await fetch('/api/docs/generate/' + leadId, {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({ type })
                });
                const data = await res.json();
                if (data.success) {
                    showToast(label + ' ready! Downloading...', 'success');
                    window.location.href = data.download;
                } else {
                    showToast('Error: ' + (data.error || 'Generation failed'), 'error');
                }
            } catch (err) {
                console.error('Doc generation error:', err);
                showToast('Failed to generate document', 'error');
            }
        }

        async function exportIDML(leadId) {
            try {
                showToast('Generating InDesign file...', 'info');
                const res = await fetch('/api/idml/generate/' + leadId, {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' }
                });
                const data = await res.json();
                if (data.success) {
                    showToast('IDML ready! Downloading...', 'success');
                    window.location.href = '/api/idml/download/' + data.filename;
                } else {
                    showToast('Error: ' + (data.error || 'Generation failed'), 'error');
                }
            } catch (err) {
                console.error('IDML export error:', err);
                showToast('Failed to generate IDML file', 'error');
            }
        }

        async function generateProposal(leadId) {
            if (!leadId) return alert('No lead selected');
            const prepFor = prompt('Prepared for (name):', '');
            if (prepFor === null) return;
            const prepCompany = prompt('Company name:', '');
            try {
                const btn = event?.target?.closest('button');
                if (btn) btn.innerHTML = '<i class="bi bi-hourglass-split"></i> Generating...';
                const res = await fetch('/api/crm/leads/' + leadId + '/proposal', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({ prepared_for: prepFor, prepared_for_company: prepCompany })
                });
                if (!res.ok) throw new Error('Failed to generate');
                const blob = await res.blob();
                const url = URL.createObjectURL(blob);
                const a = document.createElement('a');
                a.href = url;
                a.download = 'Proposal_' + (prepCompany || 'CRE') + '.docx';
                a.click();
                URL.revokeObjectURL(url);
                if (btn) btn.innerHTML = '<i class="bi bi-file-earmark-richtext"></i> Generate Proposal';
            } catch(e) { alert('Proposal generation failed: ' + e.message); }
        }
        async function createFlyer(leadId) {
            flyerLeadId = leadId;
            flyerPhotos = [];
            // First create the draft
            try {
                const res = await fetch(`/api/marketing/create-from-crm/${leadId}`, {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' }
                });
                const data = await res.json();
                if (data.success) {
                    flyerDraftId = data.draft_id;
                    document.getElementById('photoModalTitle').textContent = 'Upload Property Photos';
                    document.getElementById('photoPreviewGrid').innerHTML = '';
                    document.getElementById('photoFileInput').value = '';
                    document.getElementById('photoCountBadge').textContent = '0 photos';
                    document.getElementById('photoUploadModal').style.display = 'flex';
                } else {
                    showToast(data.error || 'Failed to create draft', 'error');
                }
            } catch (err) {
                showToast('Error: ' + err.message, 'error');
            }
        }

        function closePhotoModal() {
            document.getElementById('photoUploadModal').style.display = 'none';
        }

        function handlePhotoSelect(input) {
            const files = Array.from(input.files);
            flyerPhotos = [...flyerPhotos, ...files];
            renderPhotoPreview();
        }

        function removePhoto(index) {
            flyerPhotos.splice(index, 1);
            renderPhotoPreview();
        }

        function renderPhotoPreview() {
            const grid = document.getElementById('photoPreviewGrid');
            document.getElementById('photoCountBadge').textContent = `${flyerPhotos.length} photo${flyerPhotos.length !== 1 ? 's' : ''}`;
            grid.innerHTML = flyerPhotos.map((file, i) => `
                <div class="photo-preview-item">
                    <img src="${URL.createObjectURL(file)}" alt="Photo ${i + 1}">
                    <button class="photo-remove-btn" onclick="removePhoto(${i})"><i class="bi bi-x"></i></button>
                    <span class="photo-label">${file.name.substring(0, 15)}${file.name.length > 15 ? '...' : ''}</span>
                </div>
            `).join('');
        }

        async function submitPhotosAndCreate() {
            if (!flyerDraftId) return;
            const submitBtn = document.getElementById('photoSubmitBtn');
            submitBtn.disabled = true;
            submitBtn.innerHTML = '<i class="bi bi-hourglass-split"></i> Uploading...';

            try {
                // Upload photos if any
                if (flyerPhotos.length > 0) {
                    const formData = new FormData();
                    flyerPhotos.forEach(f => formData.append('photos', f));
                    const uploadRes = await fetch(`/api/marketing/drafts/${flyerDraftId}/photos`, {
                        method: 'POST',
                        body: formData
                    });
                    const uploadData = await uploadRes.json();
                    if (!uploadData.success) {
                        showToast('Photo upload failed: ' + (uploadData.error || 'Unknown error'), 'error');
                        submitBtn.disabled = false;
