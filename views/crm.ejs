        function escapeHtml(str) {
            if (!str) return '';
            return str.replace(/&/g, '&amp;').replace(/</g, '&lt;').replace(/>/g, '&gt;');
        }

        function showToast(message, type = 'success') {
            const container = document.getElementById('toastContainer');
            if (!container) return;
            const toast = document.createElement('div');
            toast.className = `toast ${type}`;
            toast.innerHTML = `<span>${type === 'success' ? '<i class="bi bi-check-circle"></i>' : '<i class="bi bi-x-circle"></i>'}</span><span>${message}</span>`;
            container.appendChild(toast);
            setTimeout(() => toast.remove(), 4000);
        }

        function openAddLeadModal() {
            document.getElementById('addLeadForm').reset();
            document.getElementById('leadState').value = 'FL';
            document.getElementById('addLeadModal').classList.add('active');
        }

        function closeAddLeadModal() {
            document.getElementById('addLeadModal').classList.remove('active');
        }

        // Danimal Data address typeahead autocomplete
        let searchTimeout;
        function setupAddressLookup() {
            const addrField = document.getElementById('leadAddress');
            if (!addrField) return;

            // Create dropdown container
            const wrapper = addrField.parentElement;
            wrapper.style.position = 'relative';
            const dropdown = document.createElement('div');
            dropdown.id = 'addressDropdown';
            dropdown.style.cssText = 'display:none;position:absolute;top:100%;left:0;right:0;background:var(--bg-surface);border:1px solid var(--border);border-radius:0 0 8px 8px;box-shadow:0 8px 24px rgba(0,0,0,0.15);z-index:9999;max-height:280px;overflow-y:auto;';
            wrapper.appendChild(dropdown);

            addrField.addEventListener('input', function() {
                clearTimeout(searchTimeout);
                const q = this.value.trim();
                if (q.length < 3) { dropdown.style.display = 'none'; return; }
                searchTimeout = setTimeout(() => searchAddresses(q), 250);
            });

            addrField.addEventListener('blur', () => {
                setTimeout(() => { dropdown.style.display = 'none'; }, 200);
            });

            addrField.addEventListener('keydown', (e) => {
                const items = dropdown.querySelectorAll('.addr-item');
                const active = dropdown.querySelector('.addr-item.active');
                if (e.key === 'ArrowDown') {
                    e.preventDefault();
                    if (!active && items.length) items[0].classList.add('active');
                    else if (active && active.nextElementSibling) { active.classList.remove('active'); active.nextElementSibling.classList.add('active'); }
                } else if (e.key === 'ArrowUp') {
                    e.preventDefault();
                    if (active && active.previousElementSibling) { active.classList.remove('active'); active.previousElementSibling.classList.add('active'); }
                } else if (e.key === 'Enter') {
                    e.preventDefault();
                    if (active) active.click();
                    else if (items.length === 1) items[0].click();
                }
            });
        }

        async function searchAddresses(q) {
            const dropdown = document.getElementById('addressDropdown');
            try {
                const res = await fetch('/api/danimal/address-search?q=' + encodeURIComponent(q));
                const data = await res.json();
                if (!data.results || data.results.length === 0) {
                    dropdown.innerHTML = '<div style="padding:12px;color:var(--text-muted);font-size:12px;">No matches found</div>';
                    dropdown.style.display = 'block';
                    return;
                }
                dropdown.innerHTML = data.results.map(function(r, i) {
                    var typeLabel = r.type ? r.type.substring(0, 25) : '';
                    var meta = [r.city, r.zip, typeLabel].filter(Boolean).join(' \u00B7 ');
                    return '<div class="addr-item" data-index="' + i + '" onmousedown="selectAddress(' + i + ')" style="padding:10px 14px;cursor:pointer;border-bottom:1px solid var(--border);transition:background 0.1s;" onmouseover="this.style.background=\'var(--bg-surface-alt)\'" onmouseout="this.classList.contains(\'active\')?null:this.style.background=\'transparent\'">'
                        + '<div style="font-size:13px;font-weight:600;color:var(--text-primary);">' + r.address + '</div>'
                        + '<div style="font-size:11px;color:var(--text-muted);margin-top:2px;">' + meta + (r.sqft ? ' \u00B7 ' + Math.round(r.sqft).toLocaleString() + ' SF' : '') + (r.year ? ' \u00B7 Built ' + r.year : '') + '</div>'
                        + '</div>';
                }).join('');
                window._addrResults = data.results;
                dropdown.style.display = 'block';
            } catch(e) {
                console.log('Search error:', e);
                dropdown.style.display = 'none';
            }
        }

        async function selectAddress(index) {
            var match = window._addrResults[index];
            if (!match) return;
            document.getElementById('addressDropdown').style.display = 'none';
            document.getElementById('leadAddress').value = match.address;

            // Now fetch full property data
            var params = new URLSearchParams({ address: match.address, city: match.city || '', zip: match.zip || '' });
            try {
                var res = await fetch('/api/danimal/property-lookup?' + params.toString());
                var data = await res.json();
                if (data.found && data.property) {
                    applyPropertyData(data.property);
                    showToast('Property data loaded from Danimal Data!', 'success');
                }
            } catch(e) { console.log('Lookup failed:', e); }
        }

        async function triggerLookup() {
            const addr = document.getElementById('leadAddress').value.trim();
            if (addr.length < 5) return;
            const city = document.getElementById('leadCity').value.trim();
            const zip = document.getElementById('leadZip').value.trim();

            try {
                const params = new URLSearchParams({ address: addr });
                if (city) params.append('city', city);
                if (zip) params.append('zip', zip);

                const res = await fetch('/api/danimal/property-lookup?' + params.toString());
                const data = await res.json();

                if (data.found && data.property) {
                    applyPropertyData(data.property);
                    showToast('Property data found in Danimal Data!', 'success');
                }
            } catch (err) {
                console.log('Lookup failed:', err);
            }
        }

        function applyPropertyData(p) {
            // Address fields
            if (p.city) document.getElementById('leadCity').value = titleCase(p.city);
            if (p.state) document.getElementById('leadState').value = p.state;
            if (p.zip) document.getElementById('leadZip').value = p.zip;

            // Property details
            if (p.building_sf) document.getElementById('leadSqft').value = Math.round(p.building_sf);
            if (p.lot_size_acres) {
                const lotField = document.getElementById('leadLotSize');
                if (lotField) lotField.value = p.lot_size_acres;
            }
            if (p.year_built) {
                const ybField = document.getElementById('leadYearBuilt');
                if (ybField) ybField.value = p.year_built;
            }
            if (p.zoning) {
                const zField = document.getElementById('leadZoning');
                if (zField) zField.value = p.zoning;
            }

            // Map property use to our property types
            if (p.property_type) {
                const typeMap = {
                    'RETAIL': 'Retail', 'STORE': 'Retail', 'SHOPPING': 'Retail', 'RESTAURANT': 'Retail',
                    'OFFICE': 'Office', 'PROFESSIONAL': 'Office',
                    'WAREHOUSE': 'Industrial', 'INDUSTRIAL': 'Industrial', 'MANUFACTURING': 'Industrial',
                    'MULTIFAMILY': 'Multifamily', 'APARTMENT': 'Multifamily', 'CONDO': 'Multifamily',
                    'VACANT': 'Land', 'AGRICULTURAL': 'Land',
                    'MIXED': 'Mixed-Use',
                    'HOTEL': 'Hospitality', 'MOTEL': 'Hospitality',
                    'MEDICAL': 'Medical', 'HOSPITAL': 'Medical', 'CLINIC': 'Medical',
                    'STORAGE': 'Self-Storage'
                };
                const upperType = p.property_type.toUpperCase();
                for (const [key, val] of Object.entries(typeMap)) {
                    if (upperType.includes(key)) {
                        document.getElementById('leadPropertyType').value = val;
                        break;
                    }
                }
            }

            // Auto-fill value from assessed value if no value set
            const valueField = document.getElementById('leadValue');
            if ((!valueField.value || valueField.value === '0') && p.just_value) {
                valueField.value = Math.round(p.just_value);
            }

            // Store owner info in notes if available
            if (p.owner_name) {
                const notesField = document.getElementById('leadNotes');
                const ownerInfo = 'Owner: ' + p.owner_name + (p.owner_name_2 ? ' / ' + p.owner_name_2 : '') +
                    '\nParcel: ' + (p.strap || p.parcel_id || 'N/A') +
                    (p.last_sale_price ? '\nLast Sale: $' + p.last_sale_price.toLocaleString() + (p.last_sale_date ? ' (' + new Date(p.last_sale_date).toLocaleDateString() + ')' : '') : '') +
                    (p.legal_description ? '\nLegal: ' + p.legal_description.substring(0, 120) : '');
                if (!notesField.value) notesField.value = ownerInfo;
                else notesField.value = ownerInfo + '\n---\n' + notesField.value;
            }
        }

        function titleCase(str) {
            return str.toLowerCase().replace(/\b\w/g, l => l.toUpperCase());
        }

        document.addEventListener('DOMContentLoaded', setupAddressLookup);

        function toggleDealFields() {
            const clientType = document.getElementById('leadClientType').value;
            const txType = document.getElementById('leadTransactionType');
            const valueLabel = document.getElementById('valueLabel');
            const psfLabel = document.getElementById('psfLabel');
            if (clientType === 'Tenant' || clientType === 'Landlord') {
                txType.value = 'Lease';
                if (valueLabel) valueLabel.textContent = 'Annual Rent ($)';
                if (psfLabel) psfLabel.textContent = 'Rent / SF / Year';
            } else {
                if (valueLabel) valueLabel.textContent = 'Sale Price ($)';
                if (psfLabel) psfLabel.textContent = 'Price / SF';
            }
        }

        async function submitNewLead() {
            const name = document.getElementById('leadName').value.trim();
            const company = document.getElementById('leadCompany').value.trim();
            if (!name || !company) { showToast('Please fill in Contact Name and Company', 'error'); return; }
            const leadData = {
                name, company,
                email: document.getElementById('leadEmail').value.trim() || null,
                phone: document.getElementById('leadPhone').value.trim() || null,
                contact_title: document.getElementById('leadTitle') ? document.getElementById('leadTitle').value.trim() || null : null,
                client_type: document.getElementById('leadClientType').value || 'Buyer',
                transaction_type: document.getElementById('leadTransactionType').value || 'Sale',
                priority: document.getElementById('leadPriority').value || 'Warm',
                assigned_to: document.getElementById('leadAssignedTo').value ? parseInt(document.getElementById('leadAssignedTo').value) : null,
                property_address: document.getElementById('leadAddress').value.trim() || null,
                property_city: document.getElementById('leadCity').value.trim() || null,
                property_state: document.getElementById('leadState').value.trim() || 'FL',
                property_zip: document.getElementById('leadZip').value.trim() || null,
                property_type: document.getElementById('leadPropertyType').value || null,
                property_sqft: document.getElementById('leadSqft').value ? parseInt(document.getElementById('leadSqft').value) : null,
                lot_size: document.getElementById('leadLotSize') && document.getElementById('leadLotSize').value ? parseFloat(document.getElementById('leadLotSize').value) : null,
                year_built: document.getElementById('leadYearBuilt') && document.getElementById('leadYearBuilt').value ? parseInt(document.getElementById('leadYearBuilt').value) : null,
                zoning: document.getElementById('leadZoning') ? document.getElementById('leadZoning').value.trim() || null : null,
                value: document.getElementById('leadValue').value ? parseFloat(document.getElementById('leadValue').value) : null,
                price_per_sqft: document.getElementById('leadPricePerSqft') && document.getElementById('leadPricePerSqft').value ? parseFloat(document.getElementById('leadPricePerSqft').value) : null,
                stage: document.getElementById('leadStage').value || 'New',
                source: document.getElementById('leadSource').value || null,
                notes: document.getElementById('leadNotes').value.trim() || null
            };
            try {
                const res = await fetch('/api/crm/leads', { method: 'POST', headers: { 'Content-Type': 'application/json' }, body: JSON.stringify(leadData) });
                const result = await res.json();
                if (result.success) { showToast('Lead added!', 'success'); closeAddLeadModal(); setTimeout(() => window.location.reload(), 500); }
                else { showToast(result.error || 'Failed', 'error'); }
            } catch (err) { showToast('Error adding lead', 'error'); }
        }

        function toggleSearch() { showToast('Search coming soon!', 'success'); }
    

