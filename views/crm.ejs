        var currentLeadId = null;

        async function openLeadPanel(id) {
            currentLeadId = id;
            try {
                var res = await fetch('/api/crm/leads/' + id);
                var data = await res.json();
                if (!data.success) { showToast('Failed to load lead', 'error'); return; }
                var lead = data.lead;

                document.getElementById('lpTitle').textContent = lead.name || 'Lead Details';
                document.getElementById('lpName').value = lead.name || '';
                document.getElementById('lpCompany').value = lead.company || '';
                document.getElementById('lpEmail').value = lead.email || '';
                document.getElementById('lpPhone').value = lead.phone || '';
                document.getElementById('lpStage').value = lead.stage || 'New';
                document.getElementById('lpValue').value = lead.value || '';
                document.getElementById('lpSource').value = lead.source || '';
                document.getElementById('lpTags').value = (lead.tags || []).join(', ');
                document.getElementById('lpNotes').value = lead.notes || '';
                document.getElementById('lpCreated').value = lead.created_at ? new Date(lead.created_at).toLocaleString() : '';
                document.getElementById('lpAssigned').value = lead.assigned_name || 'Unassigned';

                document.getElementById('leadPanelOverlay').classList.add('active');
                document.getElementById('leadPanel').classList.add('active');
            } catch(e) { console.error('Open lead error:', e); showToast('Error loading lead', 'error'); }
        }

        function closeLeadPanel() {
            document.getElementById('leadPanelOverlay').classList.remove('active');
            document.getElementById('leadPanel').classList.remove('active');
            currentLeadId = null;
        }

        async function saveLeadDetails() {
            if (!currentLeadId) return;
            var tagsStr = document.getElementById('lpTags').value;
            var tags = tagsStr ? tagsStr.split(',').map(function(t) { return t.trim(); }).filter(Boolean) : [];
            try {
                var res = await fetch('/api/crm/leads/' + currentLeadId, {
                    method: 'PUT',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({
                        name: document.getElementById('lpName').value,
                        company: document.getElementById('lpCompany').value,
                        email: document.getElementById('lpEmail').value,
                        phone: document.getElementById('lpPhone').value,
                        stage: document.getElementById('lpStage').value,
                        value: parseFloat(document.getElementById('lpValue').value) || 0,
                        source: document.getElementById('lpSource').value,
                        tags: tags,
                        notes: document.getElementById('lpNotes').value
                    })
                });
                var data = await res.json();
                if (data.success) {
                    showToast('Lead saved!', 'success');
                    closeLeadPanel();
                    location.reload();
                } else {
                    showToast('Save failed', 'error');
                }
            } catch(e) { console.error('Save lead error:', e); showToast('Error saving lead', 'error'); }
        }

        async function deleteLead() {
            if (!currentLeadId) return;
            if (!confirm('Are you sure you want to delete this lead? This cannot be undone.')) return;
            try {
                var res = await fetch('/api/crm/leads/' + currentLeadId, { method: 'DELETE' });
                var data = await res.json();
                if (data.success) {
                    showToast('Lead deleted', 'success');
                    closeLeadPanel();
                    location.reload();
                }
            } catch(e) { showToast('Error deleting lead', 'error'); }
        }

        // Close panel with Escape key
        document.addEventListener('keydown', function(e) {
            if (e.key === 'Escape' && document.getElementById('leadPanel').classList.contains('active')) {
                closeLeadPanel();
            }
        });

    </script>
